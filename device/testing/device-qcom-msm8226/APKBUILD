pkgname=device-qcom-msm8226
pkgdesc="Generic device package for various Qualcomm MSM8226-based devices"
pkgver=1
pkgrel=0
url="https://postmarketos.org"
license="MIT"
arch="armv7"
options="!check !archcheck"
depends="
	linux-postmarketos-qcom-msm8226
	msm-firmware-loader
	postmarketos-base
	soc-qcom-msm8226
"
makedepends="devicepkg-dev"
source="
	deviceinfo
	modules-initfs
"

build() {
	devicepkg_build $startdir $pkgname
}

package() {
	devicepkg_package $startdir $pkgname
}

