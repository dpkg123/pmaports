for _i in $_fwfiles; do
	source="$source \
		$pkgname-$_commit-$_i::https://raw.githubusercontent.com/TheMuppets/proprietary_vendor_samsung/$_commit/hero-common/proprietary/etc/wifi/$_i";
done

package() {
	for _i in $_fwfiles; do
		install -Dm644 "$srcdir/$pkgname-$_commit-$_i" \
			"$pkgdir"/lib/firmware/postmarketos/$_i
	done
}

